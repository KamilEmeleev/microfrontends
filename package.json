{
  "name": "microfrontends",
  "version": "0.0.0",
  "description": "Microfrontends applications",
  "author": "Kamil Emeleev <emeleev.kamil@gmail.com> (https://github.com/KamilEmeleev)",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "pnpm install",
    "build": "turbo run build",
    "clean": "turbo run clean && rimraf -rf pnpm-lock.yaml && rimraf -rf node_modules",
    "eslint": "turbo run eslint",
    "serve": "turbo run serve",
    "start": "turbo run start",
    "dev": "node inquirer/index.js",
    "prepare": "husky install",
    "type-check": "turbo run type-check",
    "update:deps": "pnpm update --interactive --latest -r"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@module-federation/dashboard-plugin": "^1.1.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "babel-loader": "^9.1.2",
    "bundle-loader": "^0.5.6",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^5.2.7",
    "eslint-config-abdt": "^0.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^7.1.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "inquirer": "^9.1.5",
    "inquirer-table-prompt": "^0.2.1",
    "nano-staged": "^0.8.0",
    "resolve-url-loader": "^3.1.5",
    "rimraf": "^5.0.0",
    "serve": "^14.2.0",
    "style-loader": "^3.3.2",
    "turbo": "^1.9.3",
    "typescript": "^4.9.5",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.3"
  },
  "nano-staged": {
    "packages/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ornament-ui/avatars": "^0.0.1",
    "@ornament-ui/icons": "^0.0.9",
    "@ornament-ui/kit": "^0.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
