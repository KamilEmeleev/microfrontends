{
  "name": "microfrontends",
  "version": "0.0.0",
  "description": "Microfrontends applications",
  "author": "Kamil Emeleev <emeleev.kamil@gmail.com> (https://github.com/KamilEmeleev)",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "pnpm install",
    "build": "turbo run build",
    "clean": "turbo run clean && rimraf -rf node_modules",
    "eslint": "turbo run eslint",
    "serve": "turbo run serve",
    "prestart": "pnpm install",
    "start": "turbo run start",
    "dev": "node inquirer/index.js",
    "prepare": "husky install",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "eslint-config-abdt": "^0.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^5.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.13",
    "husky": "^8.0.3",
    "inquirer": "^7.3.3",
    "inquirer-table-prompt": "^0.2.1",
    "lint-staged": "^10.5.2",
    "nano-staged": "^0.8.0",
    "rimraf": "^5.0.0",
    "turbo": "^1.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.11.3"
  },
  "nano-staged": {
    "packages/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ornament-ui/kit": "^0.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
