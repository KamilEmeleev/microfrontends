{
  "name": "microfrontends",
  "version": "0.0.0",
  "description": "Microfrontends applications",
  "author": "Kamil Emeleev <emeleev.kamil@gmail.com> (https://github.com/KamilEmeleev)",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "pnpm install",
    "build": "turbo run build",
    "clean": "turbo run clean && rimraf -rf node_modules",
    "eslint": "turbo run eslint",
    "serve": "turbo run serve",
    "prestart": "pnpm install",
    "start": "turbo run start",
    "dev": "node inquirer/index.js"
  },
  "devDependencies": {
    "eslint-config-abdt": "^0.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^6.2.13",
    "husky": "^4.3.0",
    "inquirer": "^7.3.3",
    "inquirer-table-prompt": "^0.2.1",
    "lint-staged": "^10.5.2",
    "rimraf": "^5.0.0",
    "turbo": "^1.9.1",
    "webpack": "^5.79.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.11.3"
  },
  "lint-staged": {
    "packages/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "rimraf": "^5.0.0"
  }
}
